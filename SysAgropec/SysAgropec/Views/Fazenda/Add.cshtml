
@{
    ViewBag.Title = "Add";
    Layout = "~/Views/Shared/Layout.cshtml";
}
<script type="text/javascript">

    $(".file").fileinput({
        language: "pt-BR"
    });
</script>
@Styles.Render("~/bundles/fileinput")
@Scripts.Render("~/bundles/jquery-fileinput")
<h3><i class="fa fa-angle-right"></i> Cadastro de Fazendas</h3>
<div class="row mt">
    <div class="col-lg-12">
        <div class="form-panel">
            <h4 class="mb"><i class="fa fa-angle-right"></i> Preencha os campos para efetuar o cadastro.</h4>
            <form class="form-inline" role="form" enctype="multipart/form-data">
                <fieldset class="form-group">s
                    <legend>Dados da Fazenda</legend>
                    <div class="form-group">
                        <label class="control-label">CNPJ:</label>
                        <input type="text" class="form-control" id="cnpj" name="cnpj" style="width: 170px;">
                    </div>
                    <div class="form-group">
                        <label for="exampleInputPassword2">Razão Social:</label>
                        <input type="text" class="form-control" id="razaosocial" name="razaosocial" style="width: 170px;">
                    </div>
                    <div class="form-group">
                        <label for="exampleInputEmail2">Nome Fantasia:</label>
                        <input type="text" class="form-control" id="nomefantasia" name="nomefantasia" style="width: 170px;">
                    </div>
                    <div class="form-group">
                        <label for="exampleInputPassword2">Inscrição Municipal:</label>
                        <input type="text" class="form-control" id="inscricaomunicipal" name="inscricaomunicipal" style="width: 170px;">
                    </div>
                    <br /><br />
                    <div class="form-group">
                        <label for="exampleInputEmail2">Inscrição Estadual:</label>
                        <input type="text" class="form-control" id="inscricaoestadual" name="inscricaoestadual" style="width: 170px;">
                    </div>

                </fieldset>
                <br /><br />
                <fieldset class="form-group">
                    <legend>Endereço</legend>
                    <div class="form-group">
                        <label for="exampleInputPassword2">CEP: </label>
                        <input type="text" class="form-control" id="cep" name="cep" onblur="pesquisacep(this.value);">
                    </div>
                    <div class="form-group">
                        <label for="exampleInputPassword2">Logradouro: </label>
                        <input type="text" class="form-control" id="logradouro" name="logradouro">
                    </div>
                    <div class="form-group">
                        <label for="exampleInputPassword2">Nº: </label>
                        <input type="text" class="form-control" id="numero" name="numero">
                    </div>


                    <div class="form-group">
                        <label for="exampleInputPassword2">Complemento: </label>
                        <input type="text" class="form-control" id="complemento" name="complemento">
                    </div>
                    <br><br>
                    <div class="form-group">
                        <label for="exampleInputPassword2">Bairro: </label>
                        <input type="text" class="form-control" id="bairro" name="bairro">
                    </div>
                    <div class="form-group">
                        <label for="exampleInputPassword2">Cidade: </label>
                        <input type="text" class="form-control" id="cidade" name="cidade">
                    </div>
                    <div class="form-group">
                        <label for="exampleInputPassword2">Estado: </label>
                        <input type="text" class="form-control" id="estado" name="estado">
                    </div>
                </fieldset>
                <br><br>

                <label for="exampleInputPassword2">Logo: (Arquivos permitidos: JPG, GIF, PNG)</label>
                <input id="file-0a" name="logo" class="file" type="file" multiple data-min-file-count="1" accept=".jpg,.gif,.png" style="width: 190px;">

                <br><br>
                <div class="form-group">
                    <button type="submit" class="btn btn-success"><i class="fa fa-check"></i> Incluir</button>
                </div>
            </form>

        </div>
    </div>
</div>

