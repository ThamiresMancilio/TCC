@{
    ViewBag.Title = "Consulta Fazenda";
    Layout = "~/Views/Shared/Layout.cshtml";
}
@Styles.Render("~/bundles/paging")
<h3><i class="fa fa-angle-right"></i> Fazendas</h3>
<div class="row mt">
    <div class="col-lg-12">
        
        <table id="easyPaginate" class="table table-bordered table-striped table-condensed">
            <thead>

            <th>Razão Social</th>
            <th>Nome Fantasia</th>
            <th>Logradouro</th>
            <th>Logo</th>
            <th>Opções</th>

            </thead>
            <tbody>
                @foreach (var f in Model)
            {
                    <tr class="line">
                        <td>@f.Razaosocial</td>
                        <td>@f.Nomefantasia</td>
                        <td>@f.Logradouro</td>
                        <td><img src="~/upload_images/@f.Logo" width="150" height="150" /></td>
                        <td>
                            <button class="btn btn-primary btn-xs" title="Alterar Registro"><i class="fa fa-pencil"></i></button>
                            <button class="btn btn-danger btn-xs" title="Excluir Registro"><i class="fa fa-trash-o "></i></button>
                        </td>
                    </tr>}
            </tbody>
        </table>
    </div>
</div>

