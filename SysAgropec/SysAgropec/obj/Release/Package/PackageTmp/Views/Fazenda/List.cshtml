@{
    ViewBag.Title = "Consulta Fazenda";
    Layout = "~/Views/Shared/Layout.cshtml";
}
@Styles.Render("~/bundles/paging")
<h3><i class="fa fa-angle-right"></i> Fazendas</h3>
<div class="row mt">
    @foreach (var f in Model)
    {
        <div class="col-lg-4 col-md-4 col-sm-4 mb">
            <img src="~/upload_images/@f.Logo" alt="1" width="100%" height="250">
            <div class="content-panel pn">
                <div id="spotify">
                    
                    <div class="centered">
                        <h4 style="color:black">@f.Razaosocial</h4>
                        <h5 style="color:black">@f.Logradouro, @f.Numero - @f.Bairro / @f.Cidade/@f.Estado </h5>
                        <h5 style="color:black">@f.Telefone</h5>
                        <h5 style="color:black"><i class="fa fa-envelope"></i><br />@f.Email</h5>
                        @*Total de machos: <i class="fa fa-user"></i> 5 animais<br />
                        Total de fêmeas: <i class="fa fa-user"></i> 5 animais<br />
                        Total de animais: <i class="fa fa-user"></i> 5 animais<br />*@
                    </div>
                    
                </div>

                
            </div>
        </div>
    }
