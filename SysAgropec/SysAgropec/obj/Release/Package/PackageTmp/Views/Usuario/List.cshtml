
@{
    ViewBag.Title = "List";
}

<h3><i class="fa fa-angle-right"></i> Consultar usuário</h3>
<div class="row mt">
    <div class="col-lg-12">
        <div class="form-panel">
            <h4 class="mb"><i class="fa fa-angle-right"></i> Filtros</h4>
            <form class="form-inline" role="form">
                <fieldset class="form-group">
                    <legend>Filtros de pesquisa</legend>
                    <div class="form-group">
                        <label for="exampleInputPassword2">Nome:</label>
                        <input type="text" class="form-control" id="nome" name="nome" style="width: 165px;">
                    </div>

                    <div class="form-group">
                        <label for="exampleInputEmail2">Data de Cadastro:</label>
                        <input type="date" class="form-control" id="datacadastro1" name="datacadastro1" style="width: 165px;">
                    </div>
                    <div class="form-group">
                        <label for="exampleInputEmail2">até:</label>
                        <input type="date" class="form-control" id="datacadastro2" name="datacadastro2" style="width: 165px;">
                    </div>

                    <div class="form-group">
                        <button type="submit" class="btn btn-success"><i class="fa fa-check"></i> Consultar</button>
                    </div>
                    <br /><br />
                </fieldset>
            </form>

        </div><!-- /form-panel -->

        <table id="easyPaginate" class="table table-bordered table-striped table-condensed">
            <thead>

            <th>Usuário</th>
            <th>Nome</th>
            <th>Sobrenome</th>
            <th>Último acesso:</th>
            <th>Cadastrado em:</th>

            </thead>
            <tbody>
                <tr class="line">
                    <td></td>
                    <td> </td>
                    <td></td>
                    <td></td>
                    <td>
                        <button class="btn btn-success btn-xs" title="Visualizar dados"><i class="fa fa-check"></i></button>
                        <button class="btn btn-primary btn-xs" title="Alterar Registro"><i class="fa fa-pencil"></i></button>
                        <button class="btn btn-danger btn-xs" title="Excluir Registro"><i class="fa fa-trash-o "></i></button>
                    </td>
                </tr>
            </tbody>
        </table>

    </div>
</div>
